{
  "name": "Arkanum",
  "image": "ocram85/arkanum:1",
  "containerUser": "abc",
  "containerEnv": {
    "TZ": "Europe/Berlin",
    "PASSWORD": "foo",
    "SUDO_PASSWORD": "foobar",
    "DEFAULT_WORKSPACE": "/config/workspace"
  },
  "overrideCommand": false,
  "forwardPorts": [
    "8088:8443"
  ],
  "portsAttributes": {
    "8443": {
      "label": "Applicaition",
      "onAutoForward": "notify"
    }
  },
  "customizations": {
    "devpod": {
      "prebuildRepository": "ocram85/arkanum:1"
    }
  }
}
